    shim: {
      'vendor/handlebars': {
        exports: 'Handlebars'
      },
      'vendor/jquery': {
        exports: 'jQuery'
      }
    },
  };

  if (window.massrel && massrel.cachepath) {
    config.paths = massrel.cachepath.pathsForRequire();
  } else {
